<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>UNI Store - multi vendor marketplace</title>
        <meta content="width=device-width, initial-scale=1.0" name="viewport">
    
        <!-- Favicon -->
        <link href="/img/favicon.ico" rel="icon">
    
        <!-- Google Web Fonts -->
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap" rel="stylesheet">  
    
        <!-- Font Awesome -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    
        <!-- Libraries Stylesheet -->
        <link href="/css/user/animate/animate.min.css" rel="stylesheet">
        <link href="/css/user/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    
        <!-- Customized Bootstrap Stylesheet -->
        <link href="/css/user/main/css/style.css" rel="stylesheet">
    </head>

<header>
    
    <!-- Topbar Start -->
    <div class="container-fluid">
        <div class="row bg-secondary py-1 px-xl-5">
            <!--left part-->
            <div class="col-lg-6 d-none d-lg-block">
                <div class="d-inline-flex align-items-center h-100">
                    <a class="text-body mr-3 header-link" href="">About</a>
                    <a class="text-body mr-3 header-link" href="">Contact</a>
                    <a class="text-body mr-3 header-link" href="">Help</a>
                    <a class="text-body mr-3 header-link" href="">FAQs</a>
                </div>
            </div>

            <!--right part-->
            <div class="col-lg-6 text-center text-lg-right">
                <div class="d-inline-flex align-items-center">
                    <!--Sign in & Sign up section-->
                    <div class="btn-group">
                        <button type="button" class="btn btn-sm btn-light dropdown-toggle" data-toggle="dropdown">Login</button>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a href="/customer/signin"><button class="dropdown-item" type="button">Customer login</button></a>
                            <a href=""><button class="dropdown-item" type="button">Vendor login</button></a>
                            <a href=""><button class="dropdown-item" type="button">Shipper login</button></a>
                        </div>
                    </div>
                    <!--Currencies section-->
                    <div class="btn-group mx-2">
                        <button type="button" class="btn btn-sm btn-light dropdown-toggle" data-toggle="dropdown">USD</button>
                        <div class="dropdown-menu dropdown-menu-right">
                            <button class="dropdown-item" type="button">VND</button>
                        </div>
                    </div>
                    <!--Languages section-->
                    <div class="btn-group">
                        <button type="button" class="btn btn-sm btn-light dropdown-toggle" data-toggle="dropdown">ENG</button>
                        <div class="dropdown-menu dropdown-menu-right">
                            <button class="dropdown-item" type="button">VIE</button>
                        </div>
                    </div>
                </div>

                <!--Wishlist & cart-->
                <div class="d-inline-flex align-items-center d-block d-lg-none">
                    <!--Wishlist-->
                    <!--<a href="" class="btn px-0 ml-2">
                        <i class="fas fa-heart text-dark"></i>
                        <span class="badge text-dark border border-dark rounded-circle" style="padding-bottom: 2px;">0</span>
                    </a>-->
                    <!--Cart-->
                    <a href="" class="btn px-0 ml-2">
                        <i class="fas fa-shopping-cart text-dark"></i>
                        <span class="badge text-dark border border-dark rounded-circle" style="padding-bottom: 2px;">0</span>
                    </a>
                </div>
            </div>
        </div>

        <!-- middle bar section-->
        <div class="row align-items-center bg-light py-3 px-xl-5 d-none d-lg-flex">

            <!--Logo section-->
            <div class="col-lg-4">
                <a href="" class="text-decoration-none">
                    <span class="h1 text-uppercase text-primary bg-dark px-2">UNI</span>
                    <span class="h1 text-uppercase text-dark bg-primary px-2 ml-n1">Store</span>
                </a>
            </div>

            <!--search bar-->
            <div class="col-lg-4 col-6 text-left">
                <form action="">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Search for products">
                        <div class="input-group-append">
                            <span class="input-group-text bg-transparent text-primary">
                                <i class="fa fa-search"></i>
                            </span>
                        </div>
                    </div>
                </form>
            </div>

            <!--Customer service phone number (can replace this for the user account placement)-->
            <div class="col-lg-4 col-6 text-right">
                <p class="m-0">Customer Service</p>
                <h5 class="m-0">+012 345 6789</h5>
            </div>
        </div>
    </div>
    <!-- Topbar End -->


    <!-- Navbar Start -->

    <div class="container-fluid bg-dark mb-30">
        <div class="row px-xl-5">
            <!--Categories for navbar drop down-->
            <div class="col-lg-3 d-none d-lg-block">
                <a class="btn d-flex align-items-center justify-content-between bg-primary w-100" data-toggle="collapse" href="#navbar-vertical" style="height: 65px; padding: 0 30px;">
                    <h6 class="text-dark m-0"><i class="fa fa-bars mr-2"></i>Categories</h6>
                    <i class="fa fa-angle-down text-dark"></i>
                </a>
                <!--Categories content-->
                <nav class="collapse position-absolute navbar navbar-vertical navbar-light align-items-start p-0 bg-light" id="navbar-vertical" style="width: calc(100% - 30px); z-index: 999;">
                    <div class="navbar-nav w-100">
                        <!--Clothes section-->
                        <div class="nav-item dropdown dropright">
                            <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown"> Clothes <i class="fa fa-angle-right float-right mt-1"></i></a>
                            <div class="dropdown-menu position-absolute rounded-0 border-0 m-0">
                                <a href="/customer/shop" class="dropdown-item">Men's Fashion</a>
                                <a href="/customer/shop" class="dropdown-item">Women's Fashion</a>
                            </div>
                        </div>

                        <!--Electronic section-->
                        <div class="nav-item dropdown dropright">
                            <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown"> Electronic <i class="fa fa-angle-right float-right mt-1"></i></a>
                            <div class="dropdown-menu position-absolute rounded-0 border-0 m-0">
                                <a href="/customer/shop" class="dropdown-item">Accessories & Supplies</a>
                                <a href="/customer/shop" class="dropdown-item">Camera & Photo</a>
                            </div>
                        </div>

                        <!--Computers section-->
                        <div class="nav-item dropdown dropright">
                            <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown"> Computers <i class="fa fa-angle-right float-right mt-1"></i></a>
                            <div class="dropdown-menu position-absolute rounded-0 border-0 m-0">
                                <a href="/customer/shop" class="dropdown-item">Computer Accessories & Peripherals</a>
                                <a href="/customer/shop" class="dropdown-item">Computer Components</a>
                            </div>
                        </div>

                        <a href="/customer/shop" class="nav-item nav-link">View all</a>
                        
                    </div>
                </nav>
            </div>

            <!--navbar-->
            <div class="col-lg-9">
                <nav class="navbar navbar-expand-lg bg-dark navbar-dark py-3 py-lg-0 px-0">
                    <!--This is the logo part for devices with smaller screen-->
                    <a href="" class="text-decoration-none d-block d-lg-none">
                        <span class="h1 text-uppercase text-dark bg-light px-2">UNI</span>
                        <span class="h1 text-uppercase text-light bg-primary px-2 ml-n1">Store</span>
                    </a>
                    <!--Navbar icon for devices with smaller srceen-->
                    <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <!--Navbar content-->
                    <div class="collapse navbar-collapse justify-content-between" id="navbarCollapse">
                        <div class="navbar-nav mr-auto py-0">
                            <a href="/customer/homepage" class="nav-item nav-link active header-link">Home</a>
                            <a href="/customer/shop" class="nav-item nav-link header-link">Shop</a>
                            <a href="/customer/product" class="nav-item nav-link header-link">Shop Detail</a>
                            <div class="nav-item dropdown">
                                <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown">Pages <i class="fa fa-angle-down mt-1"></i></a>
                                <div class="dropdown-menu bg-primary rounded-0 border-0 m-0">
                                    <a href="/customer/cart" class="dropdown-item header-link">Shopping Cart</a>
                                    <a href="/customer/checkout" class="dropdown-item header-link">Checkout</a>
                                </div>
                            </div>
                            <a href="/customer/contact" class="nav-item nav-link header-link">Contact</a>
                        </div>

                        <!--icon for devices with smaller screen-->
                        <div class="navbar-nav ml-auto py-0 d-none d-lg-block">
                            <!--Wishlist-->
                            <!--<a href="" class="btn px-0">
                                <i class="fas fa-heart text-primary"></i>
                                <span class="badge text-secondary border border-secondary rounded-circle" style="padding-bottom: 2px;">0</span>
                            </a>-->
                            <!--Cart-->
                            <a href="" class="btn px-0 ml-3">
                                <i class="fas fa-shopping-cart text-primary"></i>
                                <span class="badge text-secondary border border-secondary rounded-circle" style="padding-bottom: 2px;">0</span>
                            </a>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </div>
    <!-- Navbar End -->



   




</header>

</html>