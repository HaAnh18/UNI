<%- include('../vendor-partials/head.ejs') %>
<link rel="stylesheet" href="/css/vendor/orders.css"/>
<body class="vendorPage">
  <%- include('../vendor-partials/vendor-sidebar.ejs') %>

  <section class="dashboard">
    <div class="top">
      <i class="uil uil-bars sidebar-toggle"></i>
    </div> 
    
    <div class="dash-content">
  
      <div class="activity">
        <div class="title">
        <i class="bi bi-x-circle"></i>
          <span class="text">Cancelled Orders</span>
        </div>
  
        <div class="activity-data row">
          <div class="data row">
            <div class="data-list data-title col">Order ID</div>
            <div class="data-list data-title col">Customer Name</div>
            <div class="data-list data-title col">Total Price</div>
            <div class="data-list data-title badge col-sm-2">Status</div>
          </div>

          <% orders.forEach(function(order){ %>

            <div class="data">
              <div class="data-list col">#<%= order.id %></div>
              <div class="data-list col"><%= order.customerName %></div>
              <div class="data-list col"><%= order.total %> AUD</div>
              <div class="data-list col-sm-2 badge text-bg-success"><%= order.status %></div>
            </div>

          <% }); %>

      
      </div>
    </div>
  </section>
  <script src="/JS/vendor/script.js"></script>
      




</body>

<%- include('../vendor-partials/vendor-footer.ejs') %>

</html>
